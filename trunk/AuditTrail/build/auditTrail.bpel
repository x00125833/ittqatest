<?xml version="1.0" encoding="UTF-8"?>
<process
    name="auditTrail"
    targetNamespace="http://enterprise.netbeans.org/bpel/AuditTrail/auditTrail"
    xmlns:tns="http://enterprise.netbeans.org/bpel/AuditTrail/auditTrail"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://j2ee.netbeans.org/xsd/tableSchema">
    <import namespace="http://j2ee.netbeans.org/wsdl/AuditTrail/RecordAudit" location="RecordAudit.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://j2ee.netbeans.org/wsdl/AuditTrail/AuditDB" location="AuditDB.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink2" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/AuditDB" partnerLinkType="tns:jdbcpartner" partnerRole="jdbcPortTypeRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/RecordAudit" partnerLinkType="tns:RecordAudit" myRole="RecordAuditPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="InsertOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/AuditDB" messageType="tns:RetMsg"/>
        <variable name="DBInsert" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/AuditDB" messageType="tns:inputMsg"/>
        <variable name="RecordAuditOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/RecordAudit" messageType="tns:RecordAuditOperationResponse"/>
        <variable name="RecordAuditOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/RecordAudit" messageType="tns:RecordAuditOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="RecordAuditOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/RecordAudit" portType="tns:RecordAuditPortType" variable="RecordAuditOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="RecordAuditOperationIn" part="message"/>
                <to>$DBInsert.part/ns0:AUDIT_Record/ns0:MESSAGE</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="PartnerLink2" operation="insert" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/AuditDB" portType="tns:jdbcPortType" inputVariable="DBInsert" outputVariable="InsertOut"/>
        <assign name="Assign2">
            <copy>
                <from variable="InsertOut" part="part"/>
                <to variable="RecordAuditOperationOut" part="returnvalue"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="RecordAuditOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/AuditTrail/RecordAudit" portType="tns:RecordAuditPortType" variable="RecordAuditOperationOut"/>
    </sequence>
</process>

